{
    "name": "morshadun/repositorypattern",
    "description": "Laravel Service Repository Design Pattern Scaffolding.",
    "license": "MIT",
    "authors": [
        {
            "name": "Md Morshadun Nur",
            "email": "me@morshadunnur.me",
            "homepage": "https://github.com/morshadunnur"
        }
    ],
    "homepage": "https://github.com/morshadunnur",
    "keywords": ["Laravel", "RepositoryPattern"],
    "require": {
        "illuminate/support": "~5|~6|~7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Morshadun\\RepositoryPattern\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Morshadun\\RepositoryPattern\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Morshadun\\RepositoryPattern\\RepositoryPatternServiceProvider"
            ],
            "aliases": {
                "RepositoryPattern": "Morshadun\\RepositoryPattern\\Facades\\RepositoryPattern"
            }
        }
    }
}
